@{
    ViewData["Title"] = "EmployeeDetail";

}
<body>
<header class="header_part">
  
    <h4 class="container">Employee Table</h4>
</header>
<div class="container">
    <div class="row">
        <div class="col-12">
            <div class="data_table">
<table id="empDataTable"  class="table table-striped table-bordered pt-2">
    <thead>
        <tr class ="table-dark">
            <th>ESId</th>
            <th>Id</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
            <th>Department</th>
            <th>Skill</th>
            <th>Proficiency</th>
            <th>Roll</th>
           @*  <th>Password</th> *@
            @*<th>Job</th> *@
           
            <th>Edit</th>
            <th>Delete</th>

        </tr>
    </thead>
</table>
            </div>
        </div>
    </div>
</div>

<!-- Button trigger modal -->
<!-- Modal -->
<div id="EmployeeMadal" class="madal fade modal">
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h1 class="text-primary modal-title">Edit Employee</h1>
            @*<button class="btn btn-sm btn-outline-danger" aria-label="Close"></button> *@
        </div>
        <div class="modal-body">
                <form>
                    <div class="form-row">
                        <label for="EId"></label>
                        <input type="hidden" class="form-control" id="EId">
                        <label for="ESId"></label>
                        <input type="hidden" class="form-control" id="ESId">
                        <div class="">
                            <label for="FName">First name</label>
                            <input type="text" class="form-control" id="FName">
                        </div><br />
                        <div class="">
                            <label for="LName">Last name</label>
                            <input type="text" class="form-control" id="LName">
                        </div><br />
                        <div class="">
                            <label for="Email">Email</label>
                            <input type="email" class="form-control" id="Email">
                        </div><br />
                        <div class="">
                            <label for="DropDepartment">Department</label>
                            <select id="DropDepartment" class="form-control">
                                <option>Select</option>
                            </select>
                        </div><br />
                        <div class="">
                            <label for="DropEditSkill">Skill</label>
                            <select id="DropEditSkill" class="form-control">
                                <option>Select</option>
                            </select>
                        </div><br />
                        <div class="">
                            <label for="Proficiency">Proficiency Level:</label>
                            <input id="Proficiency" class="form-control">
                        </div><br />

                      
                        <button type="button" class="btn btn-primary" onclick="UpdateEmpBtn();" id="UpdateBtn">Update</button>
                    </div>
                </form>
        </div>
     
    </div>
</div>
</div>
</body>
@section scripts{
    <script src="/lib/jquery/dist/jquery.js"></script>
    <script src="/lib//jquery/dist/jquery.min.js"></script>
    <script src="/DataTable/jquery.dataTables.js"></script>
    <script src="/DataTable/jquery.dataTables.min.js"></script>
    <script src="/js/custom.js"></script>
    <script src="/js/customsc.js"></script>


    

}